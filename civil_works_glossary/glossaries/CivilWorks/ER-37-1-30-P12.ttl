    skos:inScheme ER-37-1-30-P12: ;
    skos:prefLabel "Construction, Corps of Engineers, Civil 096X3122" .

ER-37-1-30-P12:Cost_Account_Requirements a wov:GlossaryTerm,
        skos:Concept ;
    rdfs:label "Cost Account Requirements" ;
    dcterms:bibliographicCitation "https://www.publications.usace.army.mil/Portals/76/Users/182/86/2486/ER%2037-1-30.pdf#page=84" ;
