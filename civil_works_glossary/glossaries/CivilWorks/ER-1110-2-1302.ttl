ER-1110-2-1302:Risk_management_plan a wov:GlossaryTerm,
        skos:Concept ;
    rdfs:label "Risk management plan" ;
    dcterms:bibliographicCitation "https://www.publications.usace.army.mil/Portals/76/Publications/EngineerRegulations/er_1110-2-1302.pdf#page=75" ;
    pav:wasDerivedFrom "glossaries/CivilWorks/ER-1110-2-1302.yaml#term[Risk management plan]" ;
    skos:altLabel "RMP" ;
    skos:definition "A document that a project manager prepares to foresee risks, estimate impacts, and define responses to issues. 1110-2-1302.pdf" ;
    skos:inScheme ER-1110-2-1302: ;
    skos:prefLabel "Risk management plan" .

